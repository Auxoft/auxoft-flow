Subject: [PATCH] ### temp

---
 bin/af-hotfix      | 2 +-
 bin/af-nightbuild  | 3 ---
 bin/af-release     | 3 ---
 bin/af-sprintbuild | 3 ---
 4 files changed, 1 insertion(+), 10 deletions(-)

diff --git a/bin/af-hotfix b/bin/af-hotfix
index 263db1d..b71bdfd 100644
--- a/bin/af-hotfix
+++ b/bin/af-hotfix
@@ -91,7 +91,7 @@ cmd_finish() {
     die "Could not check out $RELEASE_BRANCH."
 
 
-  mssg "pull from origin/$MASTER_BRANCH into $MASTER_BRANCH"
+  mssg "pull from origin/$RELEASE_BRANCH into $RELEASE_BRANCH"
 
   git_do fetch -q origin
   git_do pull -q origin
diff --git a/bin/af-nightbuild b/bin/af-nightbuild
index 8836088..4515924 100644
--- a/bin/af-nightbuild
+++ b/bin/af-nightbuild
@@ -15,9 +15,6 @@ usage() {
 
 
 cmd_default() {
-
-
-  
   NB_BRANCH=$NIGHTBUILD_PREFIX$VERSION_C.$VERSION_D
 
   # sanity checks
diff --git a/bin/af-release b/bin/af-release
index 54ed1fe..3e64122 100644
--- a/bin/af-release
+++ b/bin/af-release
@@ -22,9 +22,6 @@ cmd_default() {
 
 
 cmd_create() {
-
-
-  
   RELEASE_BRANCH="$RELEASE_PREFIX$VERSION_A.$(($VERSION_B + 1))"
   RELEASE_TAG="$VERSION_A.$(($VERSION_B + 1)).0.0"
 
diff --git a/bin/af-sprintbuild b/bin/af-sprintbuild
index 0f30f6b..f7f39e4 100644
--- a/bin/af-sprintbuild
+++ b/bin/af-sprintbuild
@@ -15,9 +15,6 @@ usage() {
 
 
 cmd_default() {
-
-
-  
   SB_BRANCH=$SPRINTBUILD_PREFIX$VERSION_C
   SB_VERSION="$VERSION_A.$VERSION_B.$(($VERSION_C + 1))"
 
