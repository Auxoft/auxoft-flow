 5 files changed, 18 insertions(+)

diff --git a/bin/af-feature b/bin/af-feature
index 25e9480..3b88b0d 100644
--- a/bin/af-feature
+++ b/bin/af-feature
@@ -21,6 +21,8 @@ cmd_default() {
 
 
 cmd_start() {
+  parse_args "$@"
+
   if [[ "$1" = "" ]]; then
     usage
     exit 1
@@ -62,6 +64,8 @@ cmd_start() {
 
 
 cmd_finish() {
+  parse_args "$@"
+
   FEATURE_BRANCH=`git_current_branch`
 
 
diff --git a/bin/af-hotfix b/bin/af-hotfix
index 67d1213..7e09fd6 100644
--- a/bin/af-hotfix
+++ b/bin/af-hotfix
@@ -21,6 +21,8 @@ cmd_default() {
 
 
 cmd_start() {
+  parse_args "$@"
+
   if [[ "$1" = "" ]]; then
     usage
     exit 1
@@ -61,6 +63,8 @@ cmd_start() {
 
 
 cmd_finish() {
+  parse_args "$@"
+
   HOTFIX_BRANCH=`git_current_branch`
   RELEASE_BRANCH="$RELEASE_PREFIX$VERSION_A.$VERSION_B"
 
diff --git a/bin/af-nightbuild b/bin/af-nightbuild
index 4515924..c1534b9 100644
--- a/bin/af-nightbuild
+++ b/bin/af-nightbuild
@@ -15,6 +15,8 @@ usage() {
 
 
 cmd_default() {
+  parse_args "$@"
+
   NB_BRANCH=$NIGHTBUILD_PREFIX$VERSION_C.$VERSION_D
 
   # sanity checks
diff --git a/bin/af-release b/bin/af-release
index 3e64122..1016c10 100644
--- a/bin/af-release
+++ b/bin/af-release
@@ -22,6 +22,8 @@ cmd_default() {
 
 
 cmd_create() {
+  parse_args "$@"
+
   RELEASE_BRANCH="$RELEASE_PREFIX$VERSION_A.$(($VERSION_B + 1))"
   RELEASE_TAG="$VERSION_A.$(($VERSION_B + 1)).0.0"
 
@@ -94,6 +96,8 @@ cmd_create() {
 
 
 cmd_test() {
+  parse_args "$@"
+
   if [[ "$VERSION_C" == "0" ]]; then
     BUILD_BRANCH="$RCBUILD_PREFIX$VERSION"
   else
@@ -158,6 +162,8 @@ cmd_test() {
 
 
 cmd_build() {
+  parse_args "$@"
+
   RELEASEBUILD_VERSION="$VERSION_A.$VERSION_B.$(($VERSION_C + 1))"
   RELEASE_VERSION="$VERSION_A.$VERSION_B.$(($VERSION_C + 2)).0"
 
diff --git a/bin/af-sprintbuild b/bin/af-sprintbuild
index f7f39e4..dd79fdf 100644
--- a/bin/af-sprintbuild
+++ b/bin/af-sprintbuild
@@ -15,6 +15,8 @@ usage() {
 
 
 cmd_default() {
+  parse_args "$@"
+
   SB_BRANCH=$SPRINTBUILD_PREFIX$VERSION_C
   SB_VERSION="$VERSION_A.$VERSION_B.$(($VERSION_C + 1))"
 
